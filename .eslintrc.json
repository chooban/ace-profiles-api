{
  "parserOptions": {
    "ecmaVersion": 6,
    "impliedStrict": true,
    "sourceType": "module"
  },
  "env": {
    "node": true,
    "es6": true
  },
  "globals": {
  },
  "plugins": [
  ],
  "settings": {
  },
  "extends": ["airbnb"],
  "rules": {
    "comma-dangle": ["error", "never"],
    "no-underscore-dangle": 0,
    "import/no-extraneous-dependencies": ["error", {
      "devDependencies": true
    }],
    "arrow-parens": ["error", "always"],
    "import/prefer-default-export": "off",
    "class-methods-use-this": "off"
  }
}
